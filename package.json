{
  "name": "mcp-studio",
  "version": "1.0.0",
  "private": true,
  "description": "A minimal MCP (Model Context Protocol) assistant with model settings",
  "main": "./out/main/index.js",
  "author": "developer@example.com",
  "homepage": "https://github.com/example/mcp-studio",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "debug": "electron-vite -- --inspect --sourcemap",
    "build": "npm run typecheck && electron-vite build",
    "build:win": "npm run build && electron-builder --win --x64",
    "build:mac": "npm run build && electron-builder --mac --arm64 --x64",
    "build:linux": "npm run build && electron-builder --linux --x64",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "test": "vitest run --silent",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix && npm run typecheck"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "graceful-fs": "^4.2.11",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react-chartjs-2": "^5.3.0",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@anthropic-ai/sdk": "^0.41.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@eslint/js": "^9.22.0",
    "@google/genai": "^1.0.1",
    "@mistralai/mistralai": "^1.7.5",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@reduxjs/toolkit": "^2.2.5",
    "@types/node": "^22.17.1",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "antd": "^5.27.0",
    "electron": "37.3.1",
    "electron-builder": "26.0.15",
    "electron-store": "^8.2.0",
    "electron-vite": "4.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "lucide-react": "^0.525.0",
    "openai": "^5.12.2",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^14.1.2",
    "react-redux": "^9.1.2",
    "react-router": "6",
    "react-router-dom": "6",
    "styled-components": "^6.1.11",
    "typescript": "^5.6.2",
    "uuid": "^10.0.0",
    "vite": "^6.0.3",
    "vitest": "^3.2.4"
  }
}
